<h1>Title:{{movie.title}}</h1>
<h2>Genre: {{movie.genre}}</h2>
<h3>Plot: {{movie.plot}}</h3>

<ul>
  {{#each movie.cast}}
    <li>|
      {{this.name}}
      |
      {{this.occupation}}
      |
      {{this.catchPhrase}}
      |
    </li>
  {{/each}}
</ul>

<form action="/movies/{{movie._id}}/delete" method="post" id="movieId">
    <button type="submit">Delete</button>
</form>

<a href="/">HOME</a>